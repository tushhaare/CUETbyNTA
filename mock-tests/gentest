                        <div class="option-selector">
                            <div class="option-circle"></div>
                        </div>
                    </div>
                
                    <div class="option" data-option="2" onclick="selectOption(this, '6582b14f09b97a5bc25f6c61', 2)" ondblclick="clearSelection('6582b14f09b97a5bc25f6c61')">
